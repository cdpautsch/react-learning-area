{
  "name": "test-app2",
  "version": "1.0.0",
  "description": "Test app for setting up React from scratch",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config ./config/webpack.config.js --mode development",
    "start-prod": "npm run build && node src/server/server.js",
    "client-dev": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server-dev": "nodemon --config ./config/nodemon.json --watch src/server src/server/server.js",
    "dev": "concurrently \"npm run server-dev\" \"npm run client-dev\"",
    "build": "npm run lint && npm test && webpack --config ./config/webpack.config.js --mode production",
    "build-only": "webpack --config ./config/webpack.config.js --mode production",
    "lint": "npm run lint-code && npm run lint-css",
    "lint-code": "eslint src -c .eslintrc.json --ext js,jsx",
    "lint-css": "stylelint \"./src/**/*.scss\" --config ./config/.stylelintrc.json --ignore-path ./config/.stylelintignore",
    "lint-css-fix": "stylelint \"./src/**/*.scss\" --config ./config/.stylelintrc.json --ignore-path ./config/.stylelintignore --fix",
    "check-staged": "lint-staged",
    "test": "jest --config ./config/jest.config.json",
    "test:watch": "npm test -- --watch"
  },
  "keywords": ["react", "learning", "MERN"],
  "author": "cdpautsch",
  "license": "ISC",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "prettier": {
    "tabWidth": 4,
    "printWidth": 80
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{scss,sass,css}": [
      "stylelint --config ./config/.stylelintrc.json --ignore-path ./config/.stylelintignore"
    ],
    "src/**/*.{js,jsx}": [
      "eslint"
    ],
    "src/**/*.{js,jsx,json,md}": [
      "prettier --list-different"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.2",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.7",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.17.1",
    "redux-devtools": "^3.5.0",
    "sass-loader": "^7.1.0",
    "stylelint": "^10.0.1",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-prettier": "^1.1.0",
    "stylelint-scss": "^3.6.1",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "express": "^4.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "uuid": "^3.3.2"
  }
}
